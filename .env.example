# MyL CLI Configuration Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_DEFAULT_MODEL=anthropic/claude-3.5-sonnet

# MyL.zip Platform Configuration
MYL_API_BASE_URL=https://api.myl.zip
MYL_API_KEY=your_myl_platform_api_key_here

# Optional: Custom model preferences
# MYL_CHAT_MODEL=anthropic/claude-3.5-sonnet
# MYL_QUICK_MODEL=anthropic/claude-3-haiku
# MYL_COMPLEX_MODEL=anthropic/claude-3-opus

# Optional: Logging and Debug
# MYL_LOG_LEVEL=INFO
# MYL_DEBUG=false
